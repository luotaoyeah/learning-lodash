{
  "name": "@luotao/learning-lodash",
  "version": "0.1.0",
  "description": "@luotao/learning-lodash",
  "scripts": {
    "dev": "rimraf ./dist && webpack && node ./dist/index.js",
    "dev:node": "rimraf ./dist && webpack --target node && node ./dist/index.js",
    "dev:node:debug": "rimraf ./dist && webpack --target node && node %NODE_OPTIONS% ./dist/index.js",
    "build": "rimraf ./dist && webpack --mode production",
    "build:node": "rimraf ./dist && webpack --target node --mode production",
    "lint:ts": "tslint \"src/**/*.ts\""
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.137",
    "@types/node": "^12.7.2",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6"
  }
}
